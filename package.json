{
  "name": "autos",
  "version": "1.1.5",
  "description": "auto standard tooling",
  "main": "index.js",
  "keywords": [
    "auto",
    "autos",
    "standard tooling"
  ],
  "repository": "git@github.com:aotuzuche/autos.git",
  "author": "shaodahong",
  "license": "MIT",
  "bin": {
    "autos": "bin/index.js"
  },
  "scripts": {
    "release": "sh scripts/release.sh",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "eslint": "eslint bin --ext .js",
    "prettier": "prettier -c --write '**/*'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "address": "^1.0.3",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.0",
    "cache-loader": "^4.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.1",
    "cliui": "^5.0.0",
    "commander": "^3.0.0",
    "css-loader": "^2.1.1",
    "deepmerge": "^2.1.1",
    "download-git-repo": "^1.1.0",
    "ejs": "^2.7.4",
    "eslint-loader": "^2.1.1",
    "execa": "^1.0.0",
    "file-loader": "^1.1.6",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "hash-sum": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.0.0",
    "latest-version": "^4.0.0",
    "metalsmith": "^2.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.11.0",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "portfinder": "^1.0.23",
    "postcss-loader": "^2.1.5",
    "postcss-preset-env": "^5.3.0",
    "postcss-scss": "^1.0.3",
    "preload-webpack-plugin": "^3.0.0-beta.2",
    "px2rem": "^0.5.0",
    "px2rem-loader": "^0.1.9",
    "readline": "^1.3.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^6.0.6",
    "source-map-url": "^0.4.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^1.2.3",
    "thread-loader": "^2.1.2",
    "ts-loader": "^6.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "devDependencies": {
    "commitizen": "^3.1.1",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "git-cz": "^3.2.1",
    "husky": "^1.0.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.18.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
